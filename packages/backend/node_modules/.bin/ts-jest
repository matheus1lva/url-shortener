#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/matheus/Desktop/projects/url-shortener/node_modules/.pnpm/ts-jest@29.2.6_@babel+core@7.26.9_@jest+transform@29.7.0_@jest+types@29.6.3_babel-jest@_017cc4bb2aeda63434a6aa98c8e3ea96/node_modules/ts-jest/node_modules:/Users/matheus/Desktop/projects/url-shortener/node_modules/.pnpm/ts-jest@29.2.6_@babel+core@7.26.9_@jest+transform@29.7.0_@jest+types@29.6.3_babel-jest@_017cc4bb2aeda63434a6aa98c8e3ea96/node_modules:/Users/matheus/Desktop/projects/url-shortener/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/matheus/Desktop/projects/url-shortener/node_modules/.pnpm/ts-jest@29.2.6_@babel+core@7.26.9_@jest+transform@29.7.0_@jest+types@29.6.3_babel-jest@_017cc4bb2aeda63434a6aa98c8e3ea96/node_modules/ts-jest/node_modules:/Users/matheus/Desktop/projects/url-shortener/node_modules/.pnpm/ts-jest@29.2.6_@babel+core@7.26.9_@jest+transform@29.7.0_@jest+types@29.6.3_babel-jest@_017cc4bb2aeda63434a6aa98c8e3ea96/node_modules:/Users/matheus/Desktop/projects/url-shortener/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-jest/cli.js" "$@"
else
  exec node  "$basedir/../ts-jest/cli.js" "$@"
fi
